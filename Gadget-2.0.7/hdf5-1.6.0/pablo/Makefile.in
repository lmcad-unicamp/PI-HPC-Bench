PabloHDF_MPIO.o: PabloHDF_MPIO.c $(PABLO_INCLUDES) 
	$(CC) $(CFLAGS) $(CPPFLAGS) $(PABLO_FLAGS) -c $<

PabloHDFff.o: PabloHDFff.f $(PABLO_INCLUDES) 
	$(F77) $(FFLAGS) -O -c PabloHDFff.f

